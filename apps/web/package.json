{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .turbo",
    "test:e2e": "cd e2e && npx playwright test",
    "test:e2e:ui": "cd e2e && npx playwright test --ui",
    "test:e2e:headed": "cd e2e && npx playwright test --headed",
    "test:e2e:coverage": "cd e2e && COVERAGE=true npx playwright test",
    "test:e2e:update-snapshots": "cd e2e && npx playwright test --update-snapshots",
    "test:e2e:report": "cd e2e && npx playwright show-report"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@tanstack/react-router": "^1.86.6",
    "@tanstack/react-query": "^5.62.21",
    "@tanstack/react-table": "^8.20.6",
    "@tanstack/react-form": "^0.37.0",
    "@xyflow/react": "^12.3.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0",
    "lucide-react": "^0.468.0",
    "@mdx-js/rollup": "^3.1.0",
    "remark-gfm": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "rehype-pretty-code": "^0.14.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@tanstack/router-devtools": "^1.86.6",
    "@tanstack/router-plugin": "^1.86.6",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "@playwright/test": "^1.48.0",
    "playwright": "^1.48.0",
    "monocart-reporter": "^2.9.23"
  }
}
